params {
    // Bactopia
    bactopia = null
    prefix = "phyloflash"
    max_time = 1440
    max_memory = 32
    cpus = 4
    outdir = './'
    compress = false
    keep_all_files = false
    help = null
    version = null
    publish_mode = 'copy'
    force = false
    sleep_time = 5
    skip_phylogeny = false
    exclude = null
    include = null
    nfconfig = null
    condadir = "${baseDir}/conda/envs"
    cleanup_workdir = false
    registry = "dockerhub"
    singularity_cache = "$HOME/.bactopia/singularity"
    queue = 'general'
    disable_scratch = false

    // AWS Batch
    aws_region = 'us-east-1'
    aws_volumes = '/opt/conda:/mnt/conda'
    aws_cliPath = '/home/ec2-user/conda/bin/aws'
    aws_uploadStorageClass = 'STANDARD'
    aws_maxParallelTransfers = 8
    aws_delayBetweenAttempts = 15
    aws_maxTransferAttempts = 3
    aws_max_retry = 4
    aws_ecr_registry = null
    
    // bactopia tools helper
    force_rebuild = null
    skip_conda = null
    verbose = null
    silent = null

    // phyloFlash
    download_phyloflash = false
    yes = false
    phyloflash = null
    taxlevel = 6 // genus
    phyloflash_opts = ""
    allow_multiple_16s = false

    // MAFFT
    align_all = false
    mafft_opts = ""

    // IQ-TREE
    m = "MFP" // automatically determines best-fit model for your data
    bb = 1000 // Ultrafast bootstrap replicates
    alrt = 1000 // SH-like approximate likelihood ratio test (SH-aLRT) replicates
    asr = false // Ancestral state reconstruction by empirical Bayes
    iqtree_opts = ""
}
