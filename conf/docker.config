process {
    withName: 'annotate_genome|make_blastdb|blast_genes|blast_primers|blast_proteins|plasmid_blast' {
        container = "${docker_repo}/annotate_genome:${container_version}"
    }
    withName: 'update_antimicrobial_resistance|antimicrobial_resistance' {
        container = "${docker_repo}/antimicrobial_resistance:${container_version}"
    }
    withName: ariba_analysis {
        container = "${docker_repo}/ariba_analysis:${container_version}"
    }
    withName: assemble_genome {
        container = "${docker_repo}/assemble_genome:${container_version}"
    }
    withName: assembly_qc {
        container = "${docker_repo}/assembly_qc:${container_version}"
    }
    withName: 'call_variants|call_variants_auto|mapping_query' {
        container = "${docker_repo}/call_variants:${container_version}"
    }
    withName: count_31mers {
        container = "${docker_repo}/count_31mers:${container_version}"
    }
    withName: download_references {
        container = "${docker_repo}/download_references:${container_version}"
    }
    withName: 'estimate_genome_size|minmer_sketch|minmer_query' {
        container = "${docker_repo}/minmers:${container_version}"
    }
    withName: 'gather_fastqs' {
        container = "${docker_repo}/gather_fastqs:${container_version}"
    }
    withName: 'fastq_status|qc_reads|qc_original_summary|qc_final_summary' {
        container = "${docker_repo}/qc_reads:${container_version}"
    }
    withName: sequence_type {
        container = "${docker_repo}/sequence_type:${container_version}"
    }
}
