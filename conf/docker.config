process {
    withName: 'annotate_genome|make_blastdb|blast_query|plasmid_blast' {
        container = "bactopia/annotate_genome:1.1.0"
    }
    withName: 'update_antimicrobial_resistance|antimicrobial_resistance' {
        container = "bactopia/antimicrobial_resistance:1.1.0"
    }
    withName: ariba_analysis {
        container = "bactopia/ariba_analysis:1.1.0"
    }
    withName: assemble_genome {
        container = "bactopia/assemble_genome:1.1.0"
    }
    withName: 'call_variants|call_variants_auto|mapping_query' {
        container = "bactopia/call_variants:1.1.0"
    }
    withName: count_31mers {
        container = "bactopia/count_31mers:1.1.0"
    }
    withName: download_references {
        container = "bactopia/download_references:1.1.0"
    }
    withName: 'estimate_genome_size|minmer_sketch|minmer_query' {
        container = "bactopia/minmers:1.1.0"
    }
    withName: 'gather_fastqs' {
        container = "bactopia/gather_fastqs:1.1.0"
    }
    withName: insertion_sequences {
        container = "bactopia/insertion_sequences:1.1.0"
    }
    withName: 'fastq_status|qc_reads|qc_original_summary|qc_final_summary' {
        container = "bactopia/qc_reads:1.1.0"
    }
    withName: sequence_type {
        container = "bactopia/sequence_type:1.1.0"
    }
}
docker.enabled = true
