process {
    withName: 'annotate_genome|make_blastdb|blast_genes|blast_primers|blast_proteins|plasmid_blast' {
        conda = "${params.condadir}/annotate_genome-${manifest.container_version}"
    }
    withName: 'update_antimicrobial_resistance|antimicrobial_resistance' {
        conda = "${params.condadir}/antimicrobial_resistance-${manifest.container_version}"
    }
    withName: ariba_analysis {
        conda = "${params.condadir}/ariba_analysis-${manifest.container_version}"
    }
    withName: assemble_genome {
        conda = "${params.condadir}/assemble_genome-${manifest.container_version}"
    }
    withName: assembly_qc {
        conda = "${params.condadir}/assembly_qc-${manifest.container_version}"
    }
    withName: 'call_variants|call_variants_auto|mapping_query' {
        conda = "${params.condadir}/call_variants-${manifest.container_version}"
    }
    withName: count_31mers {
        conda = "${params.condadir}/count_31mers-${manifest.container_version}"
    }
    withName: download_references {
        conda = "${params.condadir}/download_references-${manifest.container_version}"
    }
    withName: 'estimate_genome_size|minmer_sketch|minmer_query' {
        conda = "${params.condadir}/minmers-${manifest.container_version}"
    }
    withName: 'gather_fastqs' {
        conda = "${params.condadir}/gather_fastqs-${manifest.container_version}"
    }
    withName: 'fastq_status|qc_reads|qc_original_summary|qc_final_summary' {
        conda = "${params.condadir}/qc_reads-${manifest.container_version}"
    }
    withName: sequence_type {
        conda = "${params.condadir}/sequence_type-${manifest.container_version}"
    }
}
